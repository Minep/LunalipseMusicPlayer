        <Label x:Name="l2" Content="0" HorizontalAlignment="Left" Height="29" Margin="118,65,0,0" VerticalAlignment="Top" Width="65" Foreground="White" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" FontSize="18"/>
        <Label x:Name="l3" Content="0" HorizontalAlignment="Left" Height="29" Margin="193,65,0,0" VerticalAlignment="Top" Width="57" Foreground="White" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" FontSize="18"/>
        <Label x:Name="l4" Content="0" HorizontalAlignment="Left" Height="29" Margin="260,65,0,0" VerticalAlignment="Top" Width="62" Foreground="White" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" FontSize="18"/>
        <Label x:Name="l5" Content="0" HorizontalAlignment="Left" Height="29" Margin="331,65,0,0" VerticalAlignment="Top" Width="60" Foreground="White" FontFamily="Microsoft YaHei UI Light" HorizontalContentAlignment="Center" FontSize="18"/>
