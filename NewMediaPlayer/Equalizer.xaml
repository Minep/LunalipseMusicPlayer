<Window x:Class="NewMediaPlayer.Equalizer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NewMediaPlayer"
        mc:Ignorable="d"
        Title="Equalizer" Height="432" Width="768" WindowStyle="None" ResizeMode="NoResize" Closing="Window_Closing">
    <Window.Background>
        <ImageBrush ImageSource="res/CrEmp.png"/>
    </Window.Background>
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="fonts.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid Background="#B2000000" MouseLeftButtonDown="Grid_MouseLeftButtonDown">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Image HorizontalAlignment="Left" Height="109" VerticalAlignment="Top" Width="91" Source="res/LunaCM.png"/>
        <Label Content="Equalizer" HorizontalAlignment="Left" Height="65" Margin="70,-5,0,0" VerticalAlignment="Top" Width="201" Foreground="White" FontSize="48" Style="{DynamicResource Equestria}"/>
        <Slider x:Name="p1" HorizontalAlignment="Left" Height="235" Margin="70,99,0,0" VerticalAlignment="Top" Width="21" ValueChanged="vol_adj_ValueChanged" Style="{DynamicResource SliderStyle1}" Maximum="100" Minimum="-100" Orientation="Vertical" SmallChange="1">
